<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="253.0" prefWidth="442.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.gui.controllers.PlansController">
    <children>
        <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
         <tabs>
            <Tab styleClass="dark-background" text="Creator">
               <content>
                  <ScrollPane fitToWidth="true" prefHeight="200.0" prefWidth="200.0" styleClass="dark-background">
                     <content>
                        <VBox fx:id="creatorVBox" alignment="CENTER" prefHeight="403.0" prefWidth="405.0" spacing="10.0" styleClass="dark-background">
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                     </content>
                  </ScrollPane>
               </content>
            </Tab>
            <Tab styleClass="dark-background" text="List">
               <content>
                  <VBox prefHeight="200.0" prefWidth="275.0" styleClass="dark-background">
                     <children>
                        <TableView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                          <columns>
                            <TableColumn prefWidth="75.0" text="Category" />
                            <TableColumn prefWidth="75.0" text="Month" />
                              <TableColumn prefWidth="75.0" text="Year" />
                              <TableColumn prefWidth="75.0" text="Value" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </content>
            </Tab>
         </tabs>
        </TabPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
