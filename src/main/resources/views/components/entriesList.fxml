<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<TableView fx:id="entries"  prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.gui.controllers.EntriesListController">
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
    <columns>
        <TableColumn fx:id="nameColumn" prefWidth="169.0" text="Name of product" />
        <TableColumn fx:id="categoryColumn" prefWidth="31.0" text="Category" />
        <TableColumn fx:id="quantityColumn" minWidth="0.0" prefWidth="89.0" text="Quantity" />
        <TableColumn fx:id="priceColumn" prefWidth="114.0" text="Price per unit" />
        <TableColumn fx:id="totalColumn" prefWidth="75.0" text="Total" />
        <TableColumn fx:id="directionColumn" prefWidth="75.0" text="Direction" />
        <TableColumn fx:id="createdAtColumn" prefWidth="75.0" text="Created At" />
    </columns>
</TableView>
