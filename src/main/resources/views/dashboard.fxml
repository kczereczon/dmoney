<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.chart.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.gui.controllers.DashboardController">
   <children>
      <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab text="Charts">
               <content>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" styleClass="dark-background">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <PieChart prefHeight="216.0" prefWidth="281.0" title="Total spend money" />
                              <PieChart />
                              <PieChart />
                           </children>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </content>
          </Tab>
            <Tab text="Today entries">
               <content>
                  <VBox fx:id="todayEntriesVBox" prefHeight="200.0" prefWidth="100.0" styleClass="dark-background">
                  </VBox>
               </content>
            </Tab>
          <Tab text="All entries">
               <content>
                  <VBox fx:id="allEntriesVBox" prefHeight="257.0" prefWidth="780.0" styleClass="dark-background">
                  </VBox>
               </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
